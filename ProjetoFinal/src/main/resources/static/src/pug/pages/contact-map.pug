extends ../layouts/app.pug

block variables
  - var pageTitle = " Contact with Map"
  - var pageName = "contact map"
  - var headerHome = false

block content
  div#contact-page
    .main-wrap
      include ../components/header/main.pug
      |
      |
      |
      // ##### FORM #####
      .container-general
        include ../components/forms/contact-map.pug
        +contact-map
      // ##### END FORM #####
      |
      |
      |
      // ##### FOOTER #####
      include ../components/footer/extend.pug
      +footer-extend(false)
        .space-top-short
          include ../components/footer/main.pug
      // ##### END FOOTER #####
